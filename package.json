{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@types/react": "^17.0.30",
        "@types/react-router-dom": "^5.3.1",
        "axios": "^0.23.0",
        "htm": "^3.1.0",
        "mobx": "^6.3.3",
        "mobx-react-lite": "^3.2.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.3.0"
    },
    "devDependencies": {
        "@svgr/webpack": "5.5.0",
        "@types/node": "^12.20.19",
        "@types/react-dom": "^17.0.9",
        "@types/webpack": "^5.28.0",
        "bfj": "^7.0.2",
        "camelcase": "^6.1.0",
        "case-sensitive-paths-webpack-plugin": "2.3.0",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.4.0",
        "css-minimizer-webpack-plugin": "^3.1.1",
        "dotenv": "8.2.0",
        "dotenv-expand": "5.1.0",
        "file-loader": "6.1.1",
        "fs-extra": "^9.0.1",
        "html-webpack-plugin": "^5.3.2",
        "identity-obj-proxy": "3.0.0",
        "mini-css-extract-plugin": "0.11.3",
        "pnp-webpack-plugin": "1.6.4",
        "postcss-flexbugs-fixes": "4.2.1",
        "postcss-loader": "3.0.0",
        "postcss-normalize": "8.0.1",
        "postcss-preset-env": "6.7.0",
        "postcss-safe-parser": "5.0.2",
        "prompts": "2.4.0",
        "resolve": "1.18.1",
        "resolve-url-loader": "^3.1.2",
        "sass-loader": "^10.0.5",
        "semver": "7.3.2",
        "style-loader": "1.3.0",
        "terser-webpack-plugin": "4.2.3",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "ts-pnp": "1.2.0",
        "typescript": "^4.3.5",
        "url-loader": "4.1.1",
        "web-vitals": "^1.1.2",
        "webpack": "^5.26.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-manifest-plugin": "^4.0.2"
    },
    "scripts": {
        "build": "yarn autoclean && webpack --config ./webpack.config.js --mode production --progress",
        "test": "node scripts/test.js",
        "clean:build": "rm -rf target/*",
        "clean:build:win": "rimraf \"public\\build\\*",
        "build:client:dev": "webpack --config ./src/frontend/config/webpack.frontend.dev.ts --watch",
        "build:server:dev": "webpack --config ./src/backend/config/webpack.backend.dev.ts --watch",
        "build:client:prod": "webpack --config ./src/frontend/config/webpack.frontend.prod.ts --mode production",
        "build:server:prod": "webpack --config ./src/backend/config/webpack.backend.prod.ts --mode production",
        "start:server:prod": "export NODE_ENV=production && node ./dist/server.*.js",
        "start:iso:dev": "export NODE_ENV=development && yarn run clean:build && yarn run build:client:dev & wait-on public/build/index-dev.html && yarn run build:server:dev --inspect",
        "start:iso:dev:win": "yarn run clean:build:win && set NODE_ENV=development&& concurrently \"yarn run build:client:dev\" \"wait-on public\\build\\index-dev.html && yarn run build:server:dev\"",
        "start:iso:prod": "yarn run build:client:prod && yarn run build:server:prod && yarn run start:server:prod",
        "heroku-prebuild": "yarn i --unsafe-perm --dev node-sass && yarn install --dev",
        "heroku-postbuild": "yarn install --dev webpack",
        "start": "yarn run start:iso:prod"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "author": "Egor Sozonov"
}

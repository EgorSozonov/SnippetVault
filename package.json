    "scripts": {
        "test": "node scripts/test.js",
        "clean:build": "rm -rf target/*",
        "build:frontend:dev": "webpack --config ./webpack.frontend.ts --mode development",
        "build:backend:dev": "webpack --config ./webpack.backend.ts --mode development",
        "build:frontend:prod": "webpack --config ./webpack.frontend.ts --mode production",
        "build:backend:prod": "webpack --config ./webpack.backend.ts --mode production",
        "start:backend:prod": "export NODE_ENV=production && node ./target/backend/snippetVaultBackend.js",
        "start:backend:dev": "node ./target/backend/snippetVaultBackend.js",
        "start:frontend:dev": "webpack serve --mode development --config ./webpack.frontend.ts",
        "start:iso:dev": "export NODE_ENV=development && yarn run clean:build && yarn run build:client:dev & wait-on public/build/index-dev.html && yarn run build:server:dev --inspect",
        "start:iso:dev:win": "yarn run clean:build:win && set NODE_ENV=development && concurrently \"yarn run build:client:dev\" \"wait-on public/build/index-dev.html && yarn run build:server:dev\"",
        "start:iso:prod": "yarn run build:client:prod && yarn run build:server:prod && yarn run start:server:prod",
        "heroku-prebuild": "yarn i --unsafe-perm --dev node-sass && yarn install --dev",
        "heroku-postbuild": "yarn install --dev webpack",
        "start": "yarn run start:frontend:dev"
    },
    "resolutions": {
        "@types/webpack": "^5.26.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
